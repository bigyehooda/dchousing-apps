<ul class="list-group">
  <% household_member.person.attribute_names.reject { |a|
    %w(created_at updated_at applicant_id id).include? a
  }.each do |attribute_name| %>
    <% case attribute_name %>
    <% when 'mail_address_id' %>
      <%= render partial: 'maybe', locals: { attribute: attribute_as_label(attribute_name), object: household_member.person.mail_address.full } %>
    <% when 'dob' %>
      <%= render partial: 'maybe', locals: { attribute: attribute_as_label(attribute_name), object: household_member.person.dob_date } %>
    <% else %>
      <%= render partial: 'maybe', locals: { attribute: attribute_as_label(attribute_name), object: household_member.person[attribute_name] } %>
    <% end %>
  <% end %>
  <%= render partial: 'maybe', locals: { attribute: 'Relationship', object: household_member.relationship } %>
</ul>
